package three1;

public class Najalnan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=60;
		int b=70;
		int c=80;
		int d=50;
		int e=30;
		
		int total=a+b+c+d+e;
		System.out.println("총합계"+total);
		
		float p = total/5;
		System.out.printf("평균"+p);
		
		System.out.println("나잘난의 성적 총합계는(total)입니다. 평균은 (p)입니다.");
				
	}

}
