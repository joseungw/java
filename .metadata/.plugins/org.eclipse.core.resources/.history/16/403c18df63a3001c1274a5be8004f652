package three1;

public class Dayif {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 5, b = 15;
		if (a > 10) {
			System.out.println("a="+a);
		}
		else
		{
			a=0;
			
		}
			System.out.println("------");
		if( b>10) {
			System.out.println("b="+b);
		}
		else 
		{
			b=0;
			System.out.println(a);
		}
	}

}
